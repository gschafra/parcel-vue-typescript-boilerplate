{
    "name": "parcel-vue-ts-test",
    "version": "0.0.1",
    "description": "parce vue typescript test project",
    "main": "index.ts",
    "scripts": {
        "dev": "parcel serve src/index.html -d dist --public-url /dist/ --open",
        "build": "cross-env NODE_ENV=production parcel build src/index.html -d build --public-url .",
        "clean": "rimraf .cache/ && rimraf dist/ && rimraf build/",
        "test:unit": "jest --no-cache",
        "test:coverage": "jest --no-cache --coverage",
        "predeploy": "npm run build"
    },
    "author": "Masonz <masonzone@gmail.com>",
    "license": "MIT",
    "devDependencies": {
        "@vue/component-compiler-utils": "^2.6.0",
        "@vue/test-utils": "^1.0.0-beta.15",
        "autoprefixer": "^9.5.0",
        "babel-core": "^6.26.0",
        "babel-jest": "^24.5.0",
        "babel-plugin-module-resolver": "^3.1.0",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-preset-es2015": "^6.24.1",
        "cross-env": "^5.1.3",
        "jest": "^24.5.0",
        "jest-serializer-vue": "^2.0.2",
        "parcel-bundler": "^1.6.2",
        "prettier": "^1.18.2",
        "rimraf": "^2.6.2",
        "rxjs-tslint-rules": "^4.25.0",
        "sass": "^1.23.0",
        "tslint": "^5.9.1",
        "tslint-config-standard": "^8.0.1",
        "tslint-microsoft-contrib": "^6.2.0",
        "typescript": "^3.3.3333",
        "vrsource-tslint-rules": "^6.0.0",
        "vue-jest": "^3.0.4",
        "vue-template-compiler": "^2.5.13",
        "vue-test-utils": "^1.0.0-beta.11"
    },
    "dependencies": {
        "css-loader": "^2.1.1",
        "semantic-ui-css": "^2.3.2",
        "semantic-ui-vue": "^0.8.1",
        "vue": "^2.5.13",
        "vue-class-component": "^7.0.1",
        "vue-hot-reload-api": "^2.3.0",
        "vue-property-decorator": "^8.0.0",
        "vue-router": "^3.0.1",
        "vuetify": "^2.1.5",
        "vuex": "^3.0.1",
        "vuex-class": "^0.3.0"
    },
    "browserslist": [
        "> 1%",
        "last 2 versions"
    ],
    "jest": {
        "moduleFileExtensions": [
            "js",
            "vue"
        ],
        "transform": {
            "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
            ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
        },
        "mapCoverage": true,
        "coverageDirectory": "./coverage/",
        "collectCoverage": true
    },
    "publishConfig": {
        "access": "public"
    }
}
