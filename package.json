{
	"name": "parcel-vue-ts-test",
	"version": "0.0.1",
	"description": "parce vue typescript test project",
	"source": "src/index.html",
	"scripts": {
		"dev": "parcel serve ./src/index.html --dist-dir ./dist --open",
		"build": "cross-env NODE_ENV=production parcel build src/index.html --no-source-maps --public-url .",
		"clean": "rimraf .cache/ && rimraf dist/ && rimraf build/",
		"compile:config": "tsc -m commonjs --esModuleInterop vue.config.ts",
		"test:unit": "jest --no-cache",
		"test:coverage": "jest --no-cache --coverage",
		"predeploy": "yarn run build"
	},
	"author": "Masonz <masonzone@gmail.com>",
	"license": "MIT",
	"devDependencies": {
		"@parcel/compressor-gzip": "^2.13.3",
		"@parcel/config-default": "^2.13.3",
		"@parcel/transformer-raw": "^2.13.3",
		"@parcel/transformer-typescript-tsc": "^2.13.3",
		"@parcel/transformer-vue": "^2.13.3",
		"@parcel/validator-typescript": "^2.13.3",
		"@stylistic/eslint-plugin": "^2.12.1",
		"@types/node": "^20.17.10",
		"@typescript-eslint/eslint-plugin": "^8.19.0",
		"@typescript-eslint/parser": "^8.19.0",
		"@vue/cli": "^5.0.8",
		"@vue/cli-service": "^5.0.8",
		"@vue/compiler-sfc": "^3.5.13",
		"@vue/component-compiler-utils": "^3.3.0",
		"@vue/eslint-config-typescript": "^13.0.0",
		"@vue/test-utils": "^2.4.6",
		"@vue/vue3-jest": "^29.2.6",
		"autoprefixer": "^10.4.20",
		"babel-core": "^6.26.3",
		"babel-jest": "^29.7.0",
		"babel-plugin-module-resolver": "^5.0.2",
		"babel-plugin-transform-runtime": "^6.23.0",
		"babel-preset-es2015": "^6.24.1",
		"cross-env": "^7.0.3",
		"eslint": "^8.57.1",
		"eslint-plugin-import": "^2.31.0",
		"eslint-plugin-import-quotes": "^0.0.1",
		"eslint-plugin-vue": "^9.32.0",
		"jest": "^29.7.0",
		"jest-serializer-vue": "^3.1.0",
		"parcel": "^2.13.3",
		"postcss": "^8.4.49",
		"prettier": "^3.4.2",
		"rimraf": "^6.0.1",
		"sass": "^1.83.0",
		"tslib": "^2.8.1",
		"typescript": "^5.7.2"
	},
	"dependencies": {
		"@primevue/themes": "^4.2.5",
		"core-js": "^3.39.0",
		"primeicons": "^7.0.0",
		"primevue": "^4.2.5",
		"tsyringe": "^4.8.0",
		"vue": "^3.5.13",
		"vue-facing-decorator": "^3.0.4",
		"vue-hot-reload-api": "^2.3.4",
		"vue-router": "^4.5.0",
		"vuex": "^4.1.0",
		"vuex-facing-decorator": "^1.2.7"
	},
	"browserslist": [
		"> 0.2%",
		"not dead"
	],
	"jest": {
		"moduleFileExtensions": [
			"js",
			"vue"
		],
		"transform": {
			"^.+\\.js$": "<rootDir>/node_modules/babel-jest",
			".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
		},
		"mapCoverage": true,
		"coverageDirectory": "./coverage/",
		"collectCoverage": true
	},
	"publishConfig": {
		"access": "public"
	},
	"pnpm": {
		"peerDependencyRules": {
			"allowedVersions": {
				"svgo": "2.8.0"
			}
		}
	},
	"volta": {
		"node": "20.18.1",
		"pnpm": "9.15.2"
	}
}
