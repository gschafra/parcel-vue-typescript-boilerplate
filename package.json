{
	"name": "parcel-vue-ts-test",
	"version": "0.0.1",
	"description": "parce vue typescript test project",
	"source": "src/index.html",
	"scripts": {
		"dev": "parcel serve ./src/index.html --dist-dir ./dist --open",
		"build": "cross-env NODE_ENV=production parcel build src/index.html --no-source-maps --public-url .",
		"clean": "rimraf .cache/ && rimraf dist/ && rimraf build/",
		"compile:config": "tsc -m commonjs --esModuleInterop vue.config.ts",
		"test:unit": "jest --no-cache",
		"test:coverage": "jest --no-cache --coverage",
		"predeploy": "yarn run build"
	},
	"author": "Masonz <masonzone@gmail.com>",
	"license": "MIT",
	"devDependencies": {
		"@parcel/compressor-gzip": "^2.7.0",
		"@parcel/transformer-typescript-tsc": "^2.7.0",
		"@parcel/transformer-vue": "^2.7.0",
		"@parcel/validator-typescript": "^2.7.0",
		"@typescript-eslint/eslint-plugin": "^5.38.1",
		"@typescript-eslint/parser": "^5.38.1",
		"@vue/cli": "^5.0.8",
		"@vue/cli-service": "^5.0.8",
		"@vue/compiler-sfc": "^3.2.40",
		"@vue/component-compiler-utils": "^3.2.2",
		"@vue/eslint-config-typescript": "^11.0.2",
		"@vue/test-utils": "^2.0.2",
		"@vue/vue3-jest": "^29.1.1",
		"autoprefixer": "^10.4.12",
		"babel-core": "^6.26.0",
		"babel-jest": "^29.1.0",
		"babel-plugin-module-resolver": "^4.1.0",
		"babel-plugin-transform-runtime": "^6.23.0",
		"babel-preset-es2015": "^6.24.1",
		"cross-env": "^7.0.3",
		"eslint": "^8.24.0",
		"eslint-plugin-import": "^2.25.2",
		"eslint-plugin-vue": "^9.5.1",
		"jest": "^29.1.1",
		"jest-serializer-vue": "^2.0.2",
		"parcel": "^2.7.0",
		"postcss": "^8.4.16",
		"prettier": "^2.7.1",
		"rimraf": "^3.0.2",
		"sass": "^1.55.0",
		"typescript": "^4.8.4"
	},
	"dependencies": {
		"core-js": "^3.25.3",
		"primeicons": "^5",
		"primevue": "^3.17.0",
		"s-vuex-class": "^0.4.1",
		"tsyringe": "^4.7.0",
		"vue": "^3.2.40",
		"vue-class-component": "^8.0.0-0",
		"vue-hot-reload-api": "^2.3.4",
		"vue-property-decorator": "^10.0.0-rc.3",
		"vue-router": "^4.1.5",
		"vuex": "^4.0.2",
		"vuex-class-modules": "^1.3.0"
	},
	"browserslist": [
		"> 0.2%",
		"not dead"
	],
	"jest": {
		"moduleFileExtensions": [
			"js",
			"vue"
		],
		"transform": {
			"^.+\\.js$": "<rootDir>/node_modules/babel-jest",
			".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
		},
		"mapCoverage": true,
		"coverageDirectory": "./coverage/",
		"collectCoverage": true
	},
	"publishConfig": {
		"access": "public"
	},
	"volta": {
		"node": "16.17.1",
		"yarn": "1.22.19"
	}
}
